{
  "name": "EntreNess",
  "version": "1.0.0",
  "description": "Back end developed with GraphQL, Prisma and NodeJS",
  "main": "index.js",
  "repository": "https://github.com/BaoPham92/EntreNess",
  "author": "Bao Pham",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "heroku-postbuild": "babel src --out-dir dist --copy-files",
    "start-dev": "env-cmd ./config/dev.env nodemon src/index.js --ext js,graphql --exec babel-node",
    "get-schema": "graphql get-schema -p prisma --dotenv config/dev.env"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "env-cmd": "^8.0.2",
    "graphql": "^14.1.1",
    "graphql-cli": "^3.0.11",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.5.0",
    "nodemon": "^1.18.10",
    "prisma-binding": "^2.3.3",
    "subscriptions-transport-ws": "^0.9.15"
  }
}
